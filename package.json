{
  "name": "icelandic-api",
  "version": "0.0.1",
  "description": "An api for exploring icelandic grammar",
  "main": "index.js",
  "scripts": {
    "start": "forever --minUptime 1000 --spinSleepTime 1000 index.js",
    "dev": "nodemon index | bunyan",
    "test": "mocha --require babel-core/register --require babel-polyfill --recursive ./test/suggestions/normalize.js -s 10",
    "test-ci": "MOCHA_FILE=$CIRCLE_TEST_REPORTS/results.xml mocha --require babel-core/register --require babel-polyfill --reporter mocha-junit-reporter --recursive ./test",
    "lint": "eslint .",
    "deploy": "./deploy.sh",
    "build-database": "rm -rf db && node ./database/build-database"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidblurton/icelandic-api"
  },
  "author": "David Blurton",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-node5": "^10.8.0",
    "babel-preset-stage-0": "^6.5.0",
    "compression": "^1.6.1",
    "concat-stream": "^1.5.1",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "express-bunyan-logger": "^1.2.0",
    "forever": "^0.15.1",
    "level-sublevel": "^6.5.4",
    "leveldown": "^1.4.4",
    "levelup": "^1.3.1",
    "leven": "^2.0.0",
    "line-by-line": "^0.1.4",
    "lodash": "^4.6.1",
    "node-statsd": "^0.1.1",
    "nodehun": "^2.0.10",
    "prfun": "^2.1.3",
    "response-time": "^2.3.1",
    "run-series": "^1.1.4",
    "traverse": "^0.6.6"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-register": "^6.7.2",
    "bunyan": "^1.7.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "6.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "jscs": "^2.11.0",
    "mocha": "^2.4.5",
    "mocha-junit-reporter": "^1.9.1",
    "nodemon": "^1.9.1"
  }
}
